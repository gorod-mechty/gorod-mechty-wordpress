block logo {
    tag: 'img',
        
    attrs: {
        var ctx = this.ctx,
            a = {};
        
        console.log(1);
        
        if (typeof ctx.src == 'string') {
            a['src'] = ctx.src
        }
        if (typeof ctx.content == 'string') {
            a['alt'] = ctx.content
        }
        if (typeof ctx.url == 'string') {
            a['href'] = ctx.url
        }

        return a;

    }
}